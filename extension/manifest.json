{
    "manifest_version": 3,
    "name": "Phishing URL Scanner",
    "version": "1.0",
    "description": "Real-time phishing detection using XGBoost ML",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["http://localhost:8000/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
}
